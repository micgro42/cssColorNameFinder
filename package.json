  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@cypress/vite-dev-server": "^1.2.2",
    "@cypress/vue": "^2.2.0",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "cypress": "^7.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "ts-jest": "^26.5.4",
    "typescript": "~4.2.3",
    "vite": "^2.1.5",
    "vue-jest": "^5.0.0-0",
    "vue-tsc": "^0.0.18"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test:unit": "jest",
    "lint": "eslint --ext .js,.ts,vue --ignore-path .gitignore --report-unused-disable-directives .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.js": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "*.yml": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
